	<queryString language="SQL">
		<![CDATA[SELECT produto.descricao AS pdescricao,
	produto.preco,
	produto.quantidade,
	fornecedor.descricao AS pfornecedor
FROM produto
	INNER JOIN fornecedor ON 
	 produto.fornecedor_codigo = fornecedor.codigo 
